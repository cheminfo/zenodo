{
  "name": "zenodo",
  "version": "2.0.0",
  "description": "Node.js library to access the Zenodo API",
  "type": "module",
  "exports": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "build": "npm run tsc && cheminfo-build --entry lib/index.js --root Zenodo",
    "build-watch": "fswatch -o src | xargs -n1 -I{} npm run build",
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run tsc",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-only && npm run eslint && npm run prettier && npm run check-types && npm run verify-build",
    "test-only": "vitest run --coverage",
    "tsc": "npm run clean && npm run tsc-build",
    "tsc-build": "tsc --project tsconfig.build.json",
    "verify-build": "npm run tsc && node lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/zenodo.git"
  },
  "keywords": [],
  "author": "MichaÃ«l Zasso",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/zenodo/issues"
  },
  "homepage": "https://github.com/cheminfo/zenodo#readme",
  "devDependencies": {
    "@types/node": "^24.0.3",
    "@vitest/coverage-v8": "^3.2.3",
    "@zakodium/tsconfig": "^1.0.2",
    "@zip.js/zip.js": "^2.7.62",
    "cheminfo-build": "^1.2.1",
    "cheminfo-types": "^1.8.1",
    "delay": "^6.0.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.29.0",
    "eslint-config-cheminfo-typescript": "^18.0.1",
    "fifo-logger": "^1.0.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "json-schema-to-ts": "^3.1.1"
  }
}
